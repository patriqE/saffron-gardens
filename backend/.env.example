# Example .env for development (DO NOT COMMIT real secrets)
DB_HOST=db
DB_PORT=5432
DB_NAME=saffrondb
DB_USERNAME=postgres
# DB_PASSWORD is expected to be provided via Docker secret or .env in development
# PRIMARY_SUPERADMIN_USERNAME and PRIMARY_SUPERADMIN_PASSWORD should be set via secrets in production
PRIMARY_SUPERADMIN_USERNAME=PatriQAdmin
# PRIMARY_SUPERADMIN_PASSWORD is kept as a secret; use a .env file locally (and add it to .gitignore) or Docker secrets in production
# PRIMARY_SUPERADMIN_PASSWORD=change-this
SPRING_PROFILES_ACTIVE=default
